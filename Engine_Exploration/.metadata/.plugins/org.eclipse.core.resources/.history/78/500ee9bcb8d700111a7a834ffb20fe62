function makeMap(mapPath)
	mapData = love.image.newImageData(mapPath)
		map = {}
		for i=0,map_w-1 do
			map[i] = {}
			for j=0,map_h-1 do
				if mapData:getPixel(i,j) == 0
					then map[i][j] = 1 --math.random(10000,1000000)%3
					--print(map[i][j])
				end
				if mapData:getPixel(i,j) == 255
					then map[i][j] = 0 --(math.random(10000,1000000)%2)+3
					--print(map[i][j])
				end
			end
		end
end